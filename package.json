{
  "name": "sad-course-bot",
  "version": "0.0.1",
  "description": "Telegram Bot on NestJS for parse and notificate exchange rates from Minfin",
  "author": "alreadyExisted",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node dist/main",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.0",
    "@nestjs/common": "^6.7.2",
    "@nestjs/config": "^0.1.0",
    "@nestjs/core": "^6.7.2",
    "@nestjs/mongoose": "^6.3.1",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/schedule": "^0.2.0",
    "cache-manager": "^2.10.1",
    "cheerio": "^1.0.0-rc.3",
    "mongoose": "^5.8.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "telegraf": "^3.36.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.9.0",
    "@nestjs/schematics": "^6.7.0",
    "@types/cheerio": "^0.22.16",
    "@types/cron": "^1.7.1",
    "@types/express": "^4.17.1",
    "@types/hapi__joi": "^16.0.8",
    "@types/mongoose": "^5.7.0",
    "@types/node": "^12.7.5",
    "cross-env": "^6.0.3",
    "minimist": "^1.2.5",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typescript": "^3.7.5"
  },
  "keywords": [
    "telegram bot",
    "nestjs",
    "parsing",
    "minfin"
  ]
}
